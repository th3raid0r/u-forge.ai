{
  "productName": "u-forge.ai",
  "version": "0.1.0",
  "identifier": "ai.u-forge.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "u-forge.ai - Universe Forge",
        "width": 1400,
        "height": 900,
        "resizable": true,
        "center": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}